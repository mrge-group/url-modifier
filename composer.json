{
  "name": "digidip/urlmodifier",
  "description": "Modifies urls based on the set of rules",
  "type": "service",
  "authors": [
    {
      "name": "Fran Lara",
      "email": "fran@digidip.net"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "spatie/url": "^1.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "phpmd/phpmd": "@stable",
    "phpstan/phpstan": "^1.4",
    "friendsofphp/php-cs-fixer": "^3"
  },
  "autoload": {
    "psr-4": {
      "digidip\\UrlModifier\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "digidip\\UrlModifier\\": "src"
    }
  }
}
